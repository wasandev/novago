{
    "name": "wasandev/billing",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Wasandev\\Billing\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wasandev\\Billing\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}